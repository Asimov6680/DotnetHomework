    $(document).ready(function(){
        content_inner_height = parseInt($(".textPageContInner").css('height'))
        $(".textPageCont").css("height",(content_inner_height+50)+'px');
        if(window.navigator.userAgent.toLowerCase().match(/MicroMessenger/i) != 'micromessenger'){
            if(making!='yuanchuang'){
                content_height = parseInt($(".textPageCont").css('height'))
                if(content_height>1000&&$(".textPageCont-footer").length>0){
                      $(".textPageCont").css("height","1000px");
                      $(".content-show-more").css("display","block");
                      $(".content-show-more-shadow").css("display","block");
                }
            }
        
            if($(".content-show-more small").length<1){
                $('.content-show-more').html('<small>点击展开全文<img src="../images/show-more2.png" style="height:16px;position:relative;top:2px;margin-left:14px;"></small>');
            }
        }
    });

    $(document).on('click','.content-show-more img',function(){
        content_inner_height = parseInt($(".textPageContInner").css('height'))
        $(".content-show-more").hide();
        $(".content-show-more-shadow").hide();
        $(".textPageCont").animate({height:(content_inner_height+50)+'px'},1000);
    });

    $(document).on('click','.content-show-more span',function(){
        content_inner_height = parseInt($(".textPageContInner").css('height'))
        $(".content-show-more").hide();
        $(".content-show-more-shadow").hide();
        $(".textPageCont").animate({height:(content_inner_height+50)+'px'},1000);
    });

